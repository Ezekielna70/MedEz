<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LandingPage1">

    <RelativeLayout
        android:id="@+id/terms_page_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/main_background"
        android:clipToOutline="true" >

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/medez_1"
            android:layout_width="199dp"
            android:layout_height="79dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="106dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="359dp"
            android:background="@drawable/logo_medez"
            android:elevation="4dp"/>
        <TextView
            android:id="@+id/organizing_"
            android:layout_width="233dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="92dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="438dp"
            android:text="@string/organizing_"
            android:textAppearance="@style/organizing_"
            android:gravity="center_horizontal|center_vertical"
            android:fontFamily="@font/capriola"
            />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/buttonmulai"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginBottom="100dp"
        android:layout_marginTop="783dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="LANJUT! ➡️"
            android:gravity="center"
            android:textAppearance="@style/ayo_mulai_p"
            android:textStyle="bold"
            android:background="@drawable/button_background"/>




    </RelativeLayout>



</androidx.coordinatorlayout.widget.CoordinatorLayout>